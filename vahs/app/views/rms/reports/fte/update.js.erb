<% if params['empfte_pg'] %>
  $('#empftes').fadeOut(1000, function() {
    $('#empftes').empty();
    $('#empftes').html('<%= j render partial: 'rms/reports/fte/empftes.html.erb', locals: { page: params['empfte_pg'], employees: @pg_empftes } %>');
    $('#empftes').fadeIn(1000);
  });
<% end %>

