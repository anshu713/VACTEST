      <% title "FY Analysis" %>
      <%= javascript_include_tag "analysis" %>
      <div id="content" class="container">
	<%= render :partial => "analysis/form" %>
	<% if @output.present? %>
        <%= render :partial => "analysis/co" if @hType == "1"  %>
        <%= render :partial => "analysis/travel" if @hType == "2"  %>
        <%= render :partial => "analysis/video" if @hType == "6"  %>
	<% end %>
	<%= render :partial => "base/error" if @err.present?  %>
      </div>
